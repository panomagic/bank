<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>testservlet</servlet-name>
        <servlet-class>servlet.TestServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>viewclients</servlet-name>
        <servlet-class>servlet.ViewClients</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>viewaccounts</servlet-name>
        <servlet-class>servlet.ViewAccounts</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>testservlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>viewclients</servlet-name>
        <url-pattern>/viewclients</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>viewaccounts</servlet-name>
        <url-pattern>/viewaccounts</url-pattern>
    </servlet-mapping>

</web-app>
